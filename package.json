{
  "name": "rocket",
  "version": "3.0.0",
  "description": "This is isomorphic boilerplate with react, redux, tests",
  "main": "webpack.config.js",
  "scripts": {
    "start": "set NODE_ENV=development&& webpack-dev-server --history-api-fallback --hot --inline --progress --colors",
    "development": "set NODE_ENV=development&& webpack",
    "production": "set NODE_ENV=production&& webpack",
    "test": "set NODE_ENV=test&& karma start karma.conf.js",
    "test:watch": "set NODE_ENV=test&& karma start karma.conf.js --watch",
    "isomorphic": "set NODE_ENV=production&& webpack&& node isomorphic/babel.server.js --harmony",
    "mocks": "node json.server.js",
    "documentation": "node_modules/.bin/jsdoc src -r -c ./.jsdoc.conf.json -d documentation/ README.md"
  },
