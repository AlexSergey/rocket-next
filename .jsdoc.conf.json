{
    "source": {
        "exclude": [ "build", "development", "documentation", "embeding", "jsdoc", "node_modules", "source/vendor", "tasks" ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "applicationName": "Demo",
        "disqus": "",
        "googleAnalytics": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "linenums": true
    },
    "plugins": ["node_modules/jsdoc-babel"],
    "babel": {
        "presets": ["es2015", "stage-0", "react"],
        "plugins": [
            "transform-decorators-legacy",
            "kneden",
            "transform-object-assign"
        ],
        "env": {
            "production": {
                "plugins": [
                    "transform-react-constant-elements",
                    "transform-react-inline-elements"
                ]
            }
        }
    }
}